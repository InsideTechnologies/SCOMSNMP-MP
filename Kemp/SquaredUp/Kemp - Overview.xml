          <!-- <Group>76a368ff-5096-fa44-bd7f-72d8acddded9</Group> -->
          <ShowItems>false</ShowItems>
          <Layouts>
            <Layout>
              <Id>performance</Id>
              <Plugin>
                <Name>Performance</Name>
                <Settings>
                  <NameValueSetting>
                    <Name>style</Name>
                    <Value>bar</Value>
                  </NameValueSetting>
                  <NameValueSetting>
                    <Name>top</Name>
                    <Value>10</Value>
                  </NameValueSetting>
                  <NameValueSetting>
                    <Name>sort</Name>
                    <Value>desc</Value>
                  </NameValueSetting>

                  <NameValueSetting>
                    <Name>object</Name>
                    <Value>Processor</Value>
                  </NameValueSetting>
                  <NameValueSetting>
                    <Name>counter</Name>
                    <Value>% Processor Time</Value>
                  </NameValueSetting>
                  <NameValueSetting>
                    <Name>instance</Name>
                    <Value></Value>
                  </NameValueSetting>				  
                  <NameValueSetting>
                    <Name>timeframe</Name>
                    <Value>Last12Hours</Value>
                  </NameValueSetting>
          <NameValueSetting>
            <Name>range</Name>
            <Value>percentage</Value>
          </NameValueSetting>
                </Settings>
              </Plugin>
            </Layout>
